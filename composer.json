{
    "name": "qsoft/seo",
    "type": "library",
    "description": "Seo angular for Laravel",
    "version": "1.0.0",
    "keywords": [
        "Qsoft",
        "Seo"
    ],
    "homepage": "https://github.com/qsoft/seo",
    "license": "MIT",
    "authors": [{
        "name": "Recycle Bin",
        "email": "thedv91@gmail.com",
        "homepage": "http://chacbumbum.github.io",
        "role": "Developer"
    }],
    "require": {
        "illuminate/support": "~5.2",
        "jonnyw/php-phantomjs": "4.*",
        "php": "~5.5|~7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "4.*",
        "scrutinizer/ocular": "~1.1",
        "squizlabs/php_codesniffer": "~2.3"
    },
    "autoload": {
        "psr-4": {
            "Qsoft\\Seo\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Qsoft\\Seo\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "post-install-cmd": [
            "PhantomInstaller\\Installer::installPhantomJS"
        ],
        "post-update-cmd": [
            "PhantomInstaller\\Installer::installPhantomJS"
        ]
    },
    "config": {
        "bin-dir": "bin"
    },
    "minimum-stability": "dev"
}